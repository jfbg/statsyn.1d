CCOMP = /usr/bin/gcc
FCOMP = /usr/local/bin/g77

.SUFFIXES: .r
.r.f:
	ratfor -C $< > $@
SUBS   = leolib.o czxlib.o

all: leolib.o leolib.a clean
#psscreendump.o 
#pscreendump.o:
#	$(FCOMP)  -c psscreendup.f

leolib.a: $(SUBS)
	ar cr leolib.a leolib.o $(SUBS)
	ranlib leolib.a
#psscreendump.o 
clean:
	rm *.o

leolib.o:  
	$(CCOMP) -c leolib.c
czxlib.o:  
	$(CCOMP) -c czxlib.c
	
